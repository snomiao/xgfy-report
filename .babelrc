{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "targets": {
                    "node": true
                }
            }
        ],
        [
            "@babel/preset-typescript"
        ]
    ],
    "plugins": [
        "@babel/plugin-syntax-top-level-await"
    ],
    "ignore": [
        "node_modules"
    ],
    "comments": false,
    "minified": false
}
